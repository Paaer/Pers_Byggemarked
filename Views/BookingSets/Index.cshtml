@model IEnumerable<ClassLibrary_Byggemarked.BookingSet>

@{
    ViewBag.Title = "Index";
}

<h2>Liste over dine reservationer</h2>

<p>
    @Html.ActionLink("Opret ny reservation", "Create", new { email = ViewBag.Email }, htmlAttributes: null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StartBooking)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlutBooking)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KundeSet.Adresse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VærktøjskatalogSet.Navn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalPris)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.StartBooking)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SlutBooking)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Status)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.KundeSet.Adresse)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.VærktøjskatalogSet.Navn)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TotalPris)
    </td>
    <td>
        @*@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |*@
        @Html.ActionLink("Details", "Details", new { id = item.Id }) 
        @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
    </td>
</tr>
}

</table>
